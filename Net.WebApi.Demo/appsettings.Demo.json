{
  // 定时任务配置
  "TimedTask": {
    "Dlls": [], // 定时任务所在程序集配置, 默认空数组
    "Cron": {} // 定时任务Cron表达式配置, 未配置将不会启用. 配置格式为: "定时任务类名":"Cron表达式"
  },

  // 中间件配置
  "Middleware": {
    "Dlls": [], // 中间件所在程序集配置, 默认空数组
    "Sort": {} // 中间件权重配置, 未配置将不会启用. 配置格式为: "中间件类名":"权重排序(越大越先)"
  },

  // webservice配置
  "WebService": {
    "Dlls": [], // webservice所在程序集配置, 默认空数组
    "Enable": {} // webservice地址配置, 未配置将不会启用. 配置格式为: "中间件接口名":"webservice访问地址.asmx"
  },

  // 跨域配置 "键":"跨域配置,..."
  "Cors": {},

  // MongoDB配置示例 "数据库名称":"连接字符串"
  "Mongo": {},

  // redis 配置 "主机地址:端口,password=密码,defaultDatabase=默认连接库"
  "Redis": []
}